<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping >
	<!--会员信息-->
	<class entity-name="vip_file" table="vip_file" dynamic-update="true">
	    <id name="id" type="string">
				<generator class="uuid.hex"></generator>
		</id>
        <!-- 会员ID -->
        <property name="vip_ID" type="string" />
        <!-- 会员姓名 -->
        <property name="vip_Name" type="string" />
        <!-- 会员密码 -->
        <property name="vip_Password" type="string" />
        <!-- 会员联系方式 -->
        <property name="vip_Telephone" type="string" />
        <!-- 会员地址 -->
        <property name="vip_Address" type="string" />
        <!-- 推荐人ID -->
        <property name="vip_Recommended_ID" type="string" />
    </class>
</hibernate-mapping>